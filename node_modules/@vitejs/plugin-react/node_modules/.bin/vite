#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/kaina/Downloads/amora-makeup-ecommerce/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_61597552e415200265e44ff54b51c03e/node_modules/vite/bin/node_modules:/mnt/c/Users/kaina/Downloads/amora-makeup-ecommerce/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_61597552e415200265e44ff54b51c03e/node_modules/vite/node_modules:/mnt/c/Users/kaina/Downloads/amora-makeup-ecommerce/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_61597552e415200265e44ff54b51c03e/node_modules:/mnt/c/Users/kaina/Downloads/amora-makeup-ecommerce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/kaina/Downloads/amora-makeup-ecommerce/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_61597552e415200265e44ff54b51c03e/node_modules/vite/bin/node_modules:/mnt/c/Users/kaina/Downloads/amora-makeup-ecommerce/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_61597552e415200265e44ff54b51c03e/node_modules/vite/node_modules:/mnt/c/Users/kaina/Downloads/amora-makeup-ecommerce/node_modules/.pnpm/vite@7.1.9_@types+node@24.7_61597552e415200265e44ff54b51c03e/node_modules:/mnt/c/Users/kaina/Downloads/amora-makeup-ecommerce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@7.1.9_@types+node@24.7_61597552e415200265e44ff54b51c03e/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@7.1.9_@types+node@24.7_61597552e415200265e44ff54b51c03e/node_modules/vite/bin/vite.js" "$@"
fi
